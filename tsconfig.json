{
	"$schema": "https://json.schemastore.org/tsconfig",
	"_version": "20",

	"compilerOptions": {
		"lib": ["es2023"],
		"target": "es2022",
		"baseUrl": "src",
		"paths": {
			"@/*": ["*"]
		},
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"module": "es2022",
		"moduleResolution": "Node"
	}
}
