{
	"compilerOptions": {
		"incremental": true,
		"outDir": "dist",
		"sourceMap": true,
		"rootDirs": ["src"],
		"baseUrl": "./src",
		"paths": {
			"@/*": ["*"]
		},
		"target": "es2022",
		"lib": ["ES2023"],
		"module": "node16",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true
	},
	"exclude": []
}
